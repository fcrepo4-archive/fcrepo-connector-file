    "storage" : {
        "binaryStorage" : {
            "type" : "transient",
            "minimumBinarySizeInBytes" : 40
        }
    },
    "security" : {
        "anonymous" : {
            "roles" : ["readonly","readwrite","admin"],
            "useOnFailedLogin" : true
        }
