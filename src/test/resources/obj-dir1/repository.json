{
    "name" : "repo",
    "jndiName" : "",
    "workspaces" : {
        "predefined" : [],
        "default" : "default",
        "allowCreation" : true
    },
    "externalSources" : {
        "federated-directory" : {
            "classname" : "org.fcrepo.connector.file.FedoraFileSystemConnector",
            "directoryPath" : "target/test-classes/obj-dir1",
            "projections" : [ "default:/federated => /" ],
            "contentBasedSha1" : "false",
            "readonly" : false,
            "extraPropertiesStorage" : "json"
        },
        "federated-directory-read-only" : {
            "classname" : "org.fcrepo.connector.file.FedoraFileSystemConnector",
            "directoryPath" : "target/test-classes/obj-dir2",
            "propertiesDirectoryPath" : "target/test-classes/obj-dir3",
            "projections" : [ "default:/readonly-federated => /" ],
            "contentBasedSha1" : "true",
            "cacheTtlSeconds" : 0,
            "readonly" : true
        },
        "fileSystem" : {
            "classname" : "org.fcrepo.connector.file.FedoraFileSystemConnector",
            "directoryPath" : "target/test-classes/test-objects",
            "readonly" : true,
            "extraPropertiesStorage": "json",
            "cacheable" : false,
            "projections" : [ "default:/files => /" ]
        }
